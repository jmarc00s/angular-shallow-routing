{
  "name": "angular-shallow-routing",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:backend": "npx json-server db/db.json",
    "start": "concurrently \"ng serve --open\" \"npm run start:backend \" ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky",
    "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{ts,css,html}\""
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,css,html}": [
      "npm run prettier:check"
    ]
  },
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "rxjs": "~7.8.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.1",
    "@angular/cli": "^17.0.1",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^8.2.2",
    "daisyui": "^4.9.0",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.0",
    "json-server": "^1.0.0-alpha.23",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "~5.2.2"
  }
}
